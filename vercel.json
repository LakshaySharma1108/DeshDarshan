{
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/html/index.html"
        },
        {
            "src": "/css/(.*)",
            "dest": "/css/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/js/$1"
        },
        {
            "src": "/html/(.*)",
            "dest": "/html/$1"
        }
    ]
}